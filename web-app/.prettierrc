{
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 120,
  "plugins": ["prettier-plugin-svelte"],
  "overrides": [
    {
      "files": ["*.css", "*.html"],
      "options": {
        "singleQuote": false
      }
    },
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
